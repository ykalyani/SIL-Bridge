<!DOCTYPE html>
<html lang="en">

<head>
    <title>Account - Bootstrap Admin Template</title>
</head>

<body>
<br>

<div class="main">

<div class="main-inner">

    <div class="container">

        <div class="row">

            <div class="span12">

                <div class="widget ">

                    <div class="widget-header">
                        <i class="icon-user"></i>

                        <h3>Add Project</h3>
                    </div>
                    <!-- /widget-header -->

                    <div class="widget-content">

                        <div class="tab-pane" id="formcontrols">
                            <form id="edit-profile" class="form-horizontal">
                                <fieldset>

                                    <div class="control-group">
                                        <label class="control-label" for="title">Title of the project</label>

                                        <div class="controls">
                                            <input type="text" class="span6" id="title" value="John">
                                        </div>
                                        <!-- /controls -->
                                    </div>
                                    <!-- /control-group -->


                                    <div class="control-group">
                                        <label class="control-label" for="describe">Description</label>

                                        <div class="controls">
                                            <input type="textarea" class="span6" id="describe" value="Donga">
                                        </div>
                                        <!-- /controls -->
                                    </div>
                                    <!-- /control-group -->


                                           <div class="control-group">
                                        <label class="control-label">Level</label>

                                        <div class="controls">
                                            <div class="btn-group">
                                                <a class="btn btn-primary" href="#"><i class=""></i>
                                                    level of studies</a>
                                                <a class="btn btn-primary dropdown-toggle" data-toggle="dropdown"
                                                   href="#"><span class="caret"></span></a>
                                                <ul class="dropdown-menu">
                                                    <li><a href="#"><i class=""></i> 2G/2S</a></li>
                                                    <li><a href="#"><i class=""></i> 3G/3S</a></li>
                                                    <li><a href="#"><i class=""></i> 4M/4S</a></li>
                                                    <li class="divider"></li>
                                                    <!--  <li><a href="#"><i class="i"></i> Make admin</a></li>     -->
                                                </ul>
                                            </div>
                                        </div>
                                        <!-- /controls -->
                                    </div>
                                    <!-- /control-group -->

                                    <div class="control-group">
                                        <label class="control-label">Duration</label>

                                        <div class="controls">
                                            <div class="btn-group">
                                                <a class="btn btn-primary" href="#"><i class=""></i> project duration</a>
                                                <a class="btn btn-primary dropdown-toggle" data-toggle="dropdown"
                                                   href="#"><span class="caret"></span></a>
                                                <ul class="dropdown-menu">
                                                    <li><a href="#"><i class=""></i> 3 months</a></li>
                                                    <li><a href="#"><i class=""></i> 6 months</a></li>
                                                    <li><a href="#"><i class=""></i> 1 year</a></li>
                                                    <li class="divider"></li>
                                                    <!--  <li><a href="#"><i class="i"></i> Make admin</a></li>     -->
                                                </ul>
                                            </div>
                                        </div>
                                        <!-- /controls -->
                                    </div>
                                    <!-- /control-group -->


                                    <div class="control-group">
                                        <label class="control-label" for="area">Project Area</label>

                                        <div class="controls">
                                            <input type="text" class="span6" id="area" value=" ">
                                        </div>
                                        <!-- /controls -->
                                    </div>
                                    <!-- /control-group -->


                                    <div class="form-actions">
                                        <button type="submit" class="btn btn-primary">Edit</button>
                                        <button type="submit" class="btn btn-primary">Submit</button>
                                    </div>
                                    <!-- /form-actions -->
                                </fieldset>
                            </form>
                        </div>
                    </div>

                </div>
            </div>
            <!-- /widget-content -->

        </div>
        <!-- /widget -->

    </div>
    <!-- /span8 -->


</div>
<!-- /row -->

</div>
<!-- /container -->

</div> <!-- /main-inner -->

</div> <!-- /main -->

</body>

</html>
